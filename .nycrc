{
	"cache": false,
	"check-coverage": false,
	"extension": [".ts"],
	"include": ["src/**/*.ts", "test/**/*.ts"],
	"exclude": ["coverage/**", "node_modules/**", "types/**", "**/*.d.ts", "**/*.test.ts"],
	"sourceMap": true,
	"reporter": ["html", "text", "text-summary"],
	"all": true,
	"instrument": true
}
